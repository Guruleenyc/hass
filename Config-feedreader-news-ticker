# RSS feed Ticker 
sensor:
  - platform: feedparser
    name: Google News
    date_format: "%a, %d %b %Y %H:%M:%S %Z"
    feed_url: "https://www.google.com/alerts/feeds/10300145326056315160/3869743766635813837"
    inclusions: title
    exclusions: []
    scan_interval:
      minutes: 15
    show_topn: 20

